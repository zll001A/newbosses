package com.shsxt.newbosses.po.vo;

import java.util.Date;

public class LocalList {

	private long noteCount;  //发帖条数
	
	private Date lastPubTime;  //最后发帖时间

	private String schLocal;  //地区序列
	
	public String getSchLocal() {
		return schLocal;
	}

	public void setSchLocal(String schLocal) {
		this.schLocal = schLocal;
	}

	public long getNoteCount() {
		return noteCount;
	}

	public void setNoteCount(long noteCount) {
		this.noteCount = noteCount;
	}

	public Date getLastPubTime() {
		return lastPubTime;
	}

	public void setLastPubTime(Date lastPubTime) {
		this.lastPubTime = lastPubTime;
	}
	
	
}
